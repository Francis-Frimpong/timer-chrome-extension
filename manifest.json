{
    "manifest_version": 3,
    "name": "Timer",
    "version": "1.0",
    "description": "This extension does amazing things.",
    "permissions": [],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["contentScript.js"],
        "css":["style.css"],
        "run_at": "document_end"
      }
    ]
  }
  